ngx_addon_name=ngx_http_insert_filter_module
ngx_addon_link=YES
if test -n "$ngx_module_link"; then
    ngx_module_type=HTTP_AUX_FILTER
    ngx_module_name=ngx_http_insert_filter_module
    ngx_module_srcs="$ngx_addon_dir/ngx_http_insert_filter_module.c"

    . auto/module
else
    HTTP_MODULES="$HTTP_MODULES ngx_http_insert_filter_module"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_http_insert_filter_module.c" 
fi
 
